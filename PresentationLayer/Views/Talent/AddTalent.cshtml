@model EntityLayer.Concrete.Talent

@{
    ViewBag.Title = "AddTalent";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>AddTalent</h2>
<br />
@using (Html.BeginForm("AddTalent", "Talent", FormMethod.Post))
{
    <div class="form-group">
        <div class="col-md-5">
            @Html.Label("Bacarıq Adı")
            @Html.TextBoxFor(t => t.TalentName, new { @class = "form-control" })
        </div>
        <br />
        <div class="col-md-5">
            @Html.Label("Nisbət")
            @Html.EditorFor(t => t.Range, new { htmlAttributes = new { @class = "form-control" } })
            <br />
        </div>

        <button class=" btn btn-outline-info">Əlavə et</button>
    </div>
}
